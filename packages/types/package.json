{
  "name": "@amplitude/rrweb-types",
  "version": "2.0.0-alpha.23",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rrweb",
    "@rrweb/types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -noEmit && vite build",
    "check-types": "tsc -noEmit",
    "prepublish": "npm run build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "homepage": "https://github.com/amplitude/rrweb/tree/main/packages/@rrweb/types#readme",
  "bugs": {
    "url": "https://github.com/amplitude/rrweb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amplitude/rrweb.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/rrweb-types.umd.cjs",
  "module": "./dist/rrweb-types.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rrweb-types.js",
      "require": "./dist/rrweb-types.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "build",
    "dist"
  ],
  "devDependencies": {
    "vite": "^3.2.0-beta.2",
    "vite-plugin-dts": "^1.7.3"
  },
  "dependencies": {
    "@amplitude/rrweb-snapshot": "^2.0.0-alpha.23"
  },
  "browserslist": [
    "supports es6-class"
  ]
}
